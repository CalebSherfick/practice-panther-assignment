# Environment Variables for Local Development
# Copy this file to .env and update values as they will overwrite appsettings.{environment}.json values

# Database Configuration (Note: adjust port if needed)
ConnectionStrings__Default=Host=localhost;Port=5433;Database=practice_panther_db;Username=postgres;Password=postgres

# JWT Configuration (REQUIRED - these override appsettings placeholders)
Jwt__Key=development-jwt-secret-key-that-is-at-least-32-characters-long-for-local-development-only
Jwt__Issuer=PracticePantherApi
Jwt__Audience=PracticePantherClient

# Frontend Configuration
VITE_API_URL=http://localhost:5206/api
